    <div class="mildliner-wrap">

        <input type="radio" id="radio-set" name="sorting" value="radio-set" checked>
        <label for="radio-set">By set</label>
        <input type="radio" id="radio-color" name="sorting" value="radio-color">
        <label for="radio-color">By color</label>

        {% set comma = joiner() %}

        <div id="by-color">
            {% set colorKeys = mildliners.color | objectKeys %}
            {% set currentColor = colorKeys[0] %}

            {% for color in colorKeys %}
                <div class="mildliner-set">
                    <h1 class="marker">{{ color }}</h1>
                    <p class="color-peek">
                        <svg class="icon" style="">
                                    <use xlink:href="#colorpeek"></use>
                                </svg> <a href="https://colorpeek.com#{% for h in mildliners.color[color] %}{{ comma() }}{{ h.color }}{% endfor %}" target="_blank">view on colorpeek</a>
                    </p>
                    {% for ml in mildliners.color[color] %}
                        <div class="mildliner">
                            <div class="mildliner-bg">
                                <svg class="icon" style="width: 100%;height:100%;color: #{{ ml.color }}">
                                    <use xlink:href="#stroke"></use>
                                </svg>
                            </div>
                            <div class="mildliner-info">
                                <h2>{{ ml.name }}</h2>
                                <p class="mildliner-color" data-color="#{{ ml.color }}">#{{ ml.color }} {% if ml.note %}{{ ml.note }}{% endif %}</p>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            {% endfor %}
        </div>

        <div id="by-set">
            {% set setKeys = mildliners.set | objectKeys %}
            {% set currentSet = setKeys[0] %}

            {% for set in setKeys %}
                <div class="mildliner-set">
                    <h1 class="marker">{{ set }}</h1>
                    <p class="color-peek">
                        <a href="https://colorpeek.com#{% for h in mildliners.set[set] %}{{ comma() }}{{ h.color }}{% endfor %}" target="_blank">
                        <svg class="icon" style="">
                                    <use xlink:href="#colorpeek"></use>
                                </svg> view on colorpeek</a>
                    </p>
                    {% for ml in mildliners.set[set] %}
                        <div class="mildliner">
                            <div class="mildliner-bg">
                                <svg class="icon" style="width: 100%;height:100%;color: #{{ ml.color }}">
                                    <use xlink:href="#stroke"></use>
                                </svg>
                            </div>
                            <div class="mildliner-info">
                                <h2>{{ ml.name }}</h2>
                                <p class="mildliner-color" data-color="#{{ ml.color }}">#{{ ml.color }} {% if ml.note %}{{ ml.note }}{% endif %}</p>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            {% endfor %}
        </div>
    </div>

